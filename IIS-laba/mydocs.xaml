<Page x:Class="IIS_laba.mydocs"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:IIS_laba"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="mydocs">

    <Grid>
        <ComboBox x:Name="docnumber" HorizontalAlignment="Left" Margin="305,142,0,0" VerticalAlignment="Top" Width="173" Height="24">
            <ComboBoxItem x:Name="first" Content="1" IsSelected="True"></ComboBoxItem>
            <ComboBoxItem x:Name="second" Content="2" IsSelected="False"></ComboBoxItem>
            <ComboBoxItem x:Name="third" Content="3" IsSelected="False"></ComboBoxItem>
            <ComboBoxItem x:Name="fourth" Content="4" IsSelected="False"></ComboBoxItem>
            <ComboBoxItem x:Name="fifth" Content="5" IsSelected="False"></ComboBoxItem>
        </ComboBox>
        <Button Click="accept_Click" Content="Підтвердити" HorizontalAlignment="Left" Margin="531,142,0,0" VerticalAlignment="Top" Width="128" Height="24"/>
        <Button Click="back_Click" Content="Назад" HorizontalAlignment="Left" Margin="531,208,0,0" VerticalAlignment="Top" Width="128" Height="42"/>
    </Grid>
</Page>
